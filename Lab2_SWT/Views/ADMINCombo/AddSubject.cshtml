@model LMMProject.Models.ComboSubject

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Create</title>
</head>
<body>

    <h4>ComboSubject</h4>
    <hr />
    <div class="row">
        <div class="col-md-4">
            <form asp-action="Create">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="ComboId" class="control-label"></label>
                    <select asp-for="ComboId" class="form-control" asp-items="ViewBag.ComboId"></select>
                </div>
                <div class="form-group">
                    <label asp-for="SubjectCode" class="control-label"></label>
                    <select asp-for="SubjectCode" class="form-control" asp-items="ViewBag.SubjectCode"></select>
                </div>
                <div class="form-group">
                    <input type="submit" value="Create" class="btn btn-primary" />
                </div>
            </form>
        </div>
    </div>

    <div>
        <a asp-action="Details" asp-route-id="@Model.ComboId">Back to List</a>
    </div>

    @section Scripts {
        @{
            await Html.RenderPartialAsync("_ValidationScriptsPartial");
        }
    }
</body>
</html>
